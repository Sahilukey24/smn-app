{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": false,
        "dockerImage": "ghcr.io/cirruslabs/flutter:stable",
        "buildCommand": "flutter build web --release",
        "outputDirectory": "build/web"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
