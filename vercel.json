{
  "buildCommand": "flutter upgrade && flutter pub get && flutter build web --release",
  "outputDirectory": "build/web",
  "devCommand": "flutter run -d chrome",
  "installCommand": "flutter pub get",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
